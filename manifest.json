{
  "manifest_version": 3,
  "name": "Popups",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://www.google.com/maps/*"],
      "js": ["content.js"],
      "css": ["bootstrap-5.3.8-dist/css/bootstrap.min.css", "content.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["mascot.png","happy.gif","sad.gif"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["activeTab"]
}
